<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="free_agent_workflow.manager.class">FreeAgent\Bundle\WorkflowBundle\Manager\Manager</parameter>
        <parameter key="free_agent_workflow.workflow.exemple.class">FreeAgent\Bundle\WorkflowBundle\Workflow\Example</parameter>
        <parameter key="free_agent_workflow.validator.exemple.class">FreeAgent\Bundle\WorkflowBundle\Validator\Example</parameter>
        <parameter key="free_agent_workflow.action.exemple.class">FreeAgent\Bundle\WorkflowBundle\Action\Example</parameter>
    </parameters>

    <services>
        <service id="free_agent_workflow.manager" class="%free_agent_workflow.manager.class%">
            <argument type="service" id="service_container" />
        </service>

        <service id="free_agent_workflow.workflow.example" class="%free_agent_workflow.workflow.exemple.class%">
            <argument type="service" id="service_container" />
        </service>

        <service id="free_agent_workflow.validator.example" class="%free_agent_workflow.validator.exemple.class%">
        </service>
        <service id="free_agent_workflow.action.example" class="%free_agent_workflow.action.exemple.class%">
        </service>
    </services>
</container>
